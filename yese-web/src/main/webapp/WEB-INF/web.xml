<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <context-param>
    <param-name>javax.faces.PROJECT_STAGE</param-name>
    <param-value>Development</param-value>
  </context-param>
  
  <context-param>
    <param-name>YeseConfig</param-name>
    <param-value>yese.properties</param-value>
  </context-param>
  
  
  <servlet>
    <servlet-name>Yese Project</servlet-name>
    <servlet-class>com.chnye.yese.servletcontext.StartupServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <listener>
	<listener-class>com.chnye.yese.servletcontext.ProjectServletContextListener</listener-class>
</listener>
</web-app>